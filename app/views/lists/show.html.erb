<h1><%= @list.name %></h1>

<h2>Bookmarks:</h2>

<ul>

<ul>
  <% @list.bookmarks.each do |bookmark| %>
    <li>
      <%= bookmark.movie.title %>
      <%= link_to 'Delete', bookmark_path(bookmark), method: :delete, data: { confirm: 'Are you sure?' } %>
    </li>
  <% end %>
</ul>


<%= link_to "New Bookmark", new_list_bookmark_path(@list) %>
